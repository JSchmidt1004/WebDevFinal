extends layout 

block content 
    table(class='mytable')
        tr 
            th Username 
            th Avatar
        each user in users
            tr 
                td=user.username
                td <img src='#{user.avatar}' width=50px height=50px/>
    div(id="center")
        canvas(id="bargraph")
    p
        a(href='/create' class='menu_item') Create Account
    p
        a(href='/login' class='menu_item menu_item_small') Login
    script(src="/script.js")
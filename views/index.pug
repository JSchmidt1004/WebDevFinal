extends layout 

block content 
    table(class='mytable')
        tr 
            th Username 
            th Email
            th Age
            th Answer 1
            th Answer 2
            th Answer 3
        each user in users
            tr 
                td=user.username
                td=user.email
                td=user.age
                td=user.answer1
                td=user.answer2
                td=user.answer3
    p
        a(href='/create' class='menu_item') Create Account
    p
        a(href='/login' class='menu_item menu_item_small') Login